  RimOrderDownPlugIn.dll RimSaleTotalPlugIn.dll RimStoragePlugIn.dll \
  RimVIPPlugIn.dll RimWsdlPlugIn.dll r3_xsm.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Shop.exe: Shop.dpr
