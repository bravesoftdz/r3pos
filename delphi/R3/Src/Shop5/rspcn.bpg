RimMsgPlugIn.dll: ..\PlugIn\Rim\RimMsgPlugIn.dpr
  $(DCC)

RimOrderDownPlugIn.dll: ..\PlugIn\Rim\RimOrderDownPlugIn.dpr
  $(DCC)

RimSaleTotalPlugIn.dll: ..\PlugIn\Rim\RimSaleTotalPlugIn.dpr
